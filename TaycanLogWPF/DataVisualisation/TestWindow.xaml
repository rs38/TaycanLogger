<Window x:Class="TaycanLogger.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
         xmlns:local="clr-namespace:TaycanLogger"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="MainGrid">

        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <local:OxyplotControl x:Name="PlotExtern2" Grid.Row="0" Grid.Column="1">
        </local:OxyplotControl>
        <Button Content="Button" HorizontalAlignment="Left" Margin="85,109,0,0" Grid.Row="1" VerticalAlignment="Top" Click="Button_ClickAsync" Height="42" Width="70"/>
    </Grid>

</Window>
    